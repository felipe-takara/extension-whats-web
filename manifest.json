{
   "background": {
   "scripts": [ "ignore-x-frame.js" ]   },
   "manifest_version": 2,
   "name": "Atalho WhatsApp",
   "permissions": [ "webRequest", "webRequestBlocking", "\u003Call_urls>" ],
   "version": "1.1",
   
